[Unit]
Description=Schwartze Ansingh letters service
After=syslog.target

[Service]
Type=simple
WorkingDirectory=/home/christine/schwartze-server
User=christine
Group=christine
ExecStart=/usr/bin/java -jar /home/christine/schwartze-server/{{schwartze__jar_name}}
StandardOutput=journal
StandardError=journal
SuccessExitStatus=143

[Install]
WantedBy=multi-user.target
